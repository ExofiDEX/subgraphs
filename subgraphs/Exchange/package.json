    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 ExofiDEX/Exchange"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.32.0",
    "@graphprotocol/graph-ts": "0.27.0"
  }
}
