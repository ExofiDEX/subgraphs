  "version": "0.0.1",
  "scripts": {
    "codegen": "mustache ../../config/$NETWORK.js template.yaml > subgraph.yaml && graph codegen",
    "build": "graph build",
    "deploy:goerli": "graph deploy --node https://api.thegraph.com/deploy/ exofidex/exchange-goerli",
