{
  "name": "Blocks",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "codegen": "mustache ../../config/$NETWORK.js template.yaml > subgraph.yaml && graph codegen",
    "build": "graph build",
    "test": "graph test -r",
    "deploy:goerli": "graph deploy --node https://api.thegraph.com/deploy/ ExofiDEX/Blocks-Goerli",
    "create-local": "graph create --node http://localhost:8020/ ExofiDEX/Blocks",
    "remove-local": "graph remove --node http://localhost:8020/ ExofiDEX/Blocks",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 ExofiDEX/Blocks"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.32.0",
    "@graphprotocol/graph-ts": "0.27.0"
  }
}
